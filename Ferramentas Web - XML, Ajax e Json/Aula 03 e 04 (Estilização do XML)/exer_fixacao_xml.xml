<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<!-- Criamos o modelo xsd -->
<!-- elemento raiz : "Aluno"-->
 <xs:element name="Aluno">
    <xs:complexType>
        <xs:sequence>
