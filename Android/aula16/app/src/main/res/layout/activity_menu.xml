<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/menu"
    android:orientation="vertical"
    android:layout_margin="30dp">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/contatos"
            android:id="@+id/btMNUContatos"/>
        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginLeft="40dp"
            android:src="@drawable/dados"
            android:id="@+id/btMNUDados"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/agendamento"
        android:id="@+id/btMNUAgendamento" />
    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginLeft="40dp"
        android:src="@drawable/consulta"
        android:id="@+id/btMNUConsultaAgendamento" />
    </LinearLayout>
</LinearLayout>
